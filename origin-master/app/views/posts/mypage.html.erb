<table>
  <thead>
    <tr>
      <th>ID</th>
      <th>Name</th>
      <th>Email</th>
      <th>Count of sign_in</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @user.each do |user| %>
      <tr>
        <td><%= user.id%>
        <td><%= user.name%>
        <td><%= user.email %></td>
         <td><%= user.sign_in_count %></td>
       
      </tr>
    <% end %>
  </tbody>
</table>

<br>